{
  "name": "@jasminejs/reporters",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasmine/reporters.git"
  },
  "keywords": [
    "test",
    "testing",
    "jasmine",
    "tdd",
    "bdd",
    "reporter"
  ],
  "scripts": {
    "posttest": "eslint && prettier --check index.js \"lib/**/*.js\" \"spec/**/*.js\"",
    "test": "jasmine",
    "cleanup": "prettier --write index.js \"lib/**/*.js\" \"spec/**/*.js\""
  },
  "description": "Reporters for the Jasmine testing framework",
  "homepage": "https://jasmine.github.io",
  "main": "./index.js",
  "exports": {
    ".": "./index.js",
    "./console": "./lib/console_reporter.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib/**/*.js",
    "package.json"
  ],
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "eslint": "^9.24.0",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-jasmine": "^4.2.2",
    "jasmine": "github:jasmine/jasmine-npm#6.0",
    "prettier": "1.17.1"
  },
  "browserslist": [
    "Safari >= 16",
    "Firefox >= 102",
    "last 2 Chrome versions",
    "last 2 Edge versions"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
